{
    "presets": ["es2015", "react", "stage-0"],
    "env": {
        "development": {
            "plugins": [
                [
                    "transform-decorators-legacy",
                    "transform-object-rest-spread"
                ],
                ["react-transform", {
                    "transforms": [{
                    "transform": "livereactload/babel-transform",
                    "imports": ["react"]
                    }]
                }]
            ]
        },
        "production": {
            "plugins": [
                [
                    "transform-decorators-legacy",
                    "transform-object-rest-spread"
                ]
            ]
        },
        "test": {
            "plugins": [
                [
                    "transform-decorators-legacy",
                    "transform-object-rest-spread"
                ]
            ]
        }
    }
}
